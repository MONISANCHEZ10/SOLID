{
  "name": "appnotes",
  "version": "1.0.0",
  "description": "conecting pod and add notes",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && npm run minify",
    "compile": "browserify src/boot.js -o dist/index.js -t [ babelify --presets [ es2015 ] ]",
    "minify": "uglifyjs --compress --mangle -- dist/index.js > dist/index.min.js",
    "develop": "gaze 'npm run compile' 'src/**/*.js'"
  },
  "author": "ms",
  "license": "ISC",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "gaze-cli": "^0.2.0",
    "uglify-js": "^3.7.0"
  },
  "dependencies": {
    "solid-auth-client": "^2.4.1"
  }
}
